{
  "name": "openai-partial-json-parser",
  "version": "1.0.0",
  "description": "Partial JSON parser extracted from OpenAI's vendored version.",
  "author": "Travis Fischer <travis@transitivebullsh.it>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/transitive-bullshit/openai-partial-json-parser.git"
  },
  "packageManager": "pnpm@9.7.0",
  "engines": {
    "node": ">=18"
  },
  "type": "module",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "del clean",
    "prebuild": "run-s clean vendor",
    "predev": "run-s clean",
    "pretest": "run-s build",
    "test": "run-s test:*",
    "test:format": "prettier --check \"**/*.{js,ts,tsx}\"",
    "test:lint": "eslint .",
    "test:typecheck": "tsc --noEmit",
    "test:unit": "vitest run",
    "prevendor": "del src/vendor",
    "vendor": "mkdir -p src/vendor/partial-json-parser && cp -r node_modules/openai/src/_vendor/partial-json-parser src/vendor",
    "release": "np",
    "precommit": "lint-staged",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky"
  },
  "devDependencies": {
    "@fisch0920/eslint-config": "^1.4.0",
    "@types/node": "^22.1.0",
    "del-cli": "^5.1.0",
    "eslint": "^8.57.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.8",
    "npm-run-all2": "^6.2.2",
    "only-allow": "^1.2.1",
    "openai": "^4.55.0",
    "prettier": "^3.3.3",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "vitest": "2.0.5"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --ignore-unknown --write",
      "eslint --fix"
    ]
  }
}
