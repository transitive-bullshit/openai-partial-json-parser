{
  "name": "openai-partial-json-parser",
  "version": "1.1.0",
  "description": "Partial JSON parser extracted from OpenAI's vendored version.",
  "author": "Travis Fischer <travis@transitivebullsh.it>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/transitive-bullshit/openai-partial-json-parser.git"
  },
  "packageManager": "pnpm@9.12.1",
  "engines": {
    "node": ">=18"
  },
  "type": "module",
  "main": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "del clean",
    "prebuild": "run-s clean vendor",
    "predev": "run-s clean",
    "pretest": "run-s build",
    "test": "run-s test:*",
    "test:format": "prettier --check \"**/*.{js,ts,tsx}\"",
    "test:lint": "eslint .",
    "test:typecheck": "tsc --noEmit",
    "test:unit": "vitest run",
    "prevendor": "del src/vendor",
    "vendor": "mkdir -p src/vendor/partial-json-parser && cp -r node_modules/openai/src/_vendor/partial-json-parser src/vendor"
  },
  "devDependencies": {
    "@fisch0920/eslint-config": "^1.4.0",
    "@types/node": "^22.7.5",
    "del-cli": "^6.0.0",
    "eslint": "^8.57.1",
    "npm-run-all2": "^6.2.3",
    "openai": "^4.67.3",
    "prettier": "^3.3.3",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3",
    "vitest": "2.1.3"
  }
}
